    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Try msnodesqlv8 Local Instance Connection",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "env": {
                "DB_HOST": "localhost",
                "DB_INSTANCE": "SQLEXPRESS",
                "DB_NAME": "databaseName",
                "DB_DRIVER": "mssql/msnodesqlv8",
                "TRUSTED_DB_CON": "true",
                "LIST_ERRORS": "true",
                "LIST_RESULTS": null
            },
            "program": "${workspaceFolder}\\index.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Try msnodesqlv8 Local Port Connection",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "env": {
                "DB_HOST": "localhost",
                "DB_PORT": "1433",
                "DB_NAME": "databaseName",
                "DB_DRIVER": "mssql/msnodesqlv8",
                "TRUSTED_DB_CON": "true",
                "LIST_ERRORS": "true",
                "LIST_RESULTS": null
            },
            "program": "${workspaceFolder}\\index.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Try Mssql Connection",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "env": {
                "DB_HOST": "192.168.132.200",
                "DB_PORT": "1433",
                "DB_NAME": "databaseName",
                "DB_USER": null,
                "DB_PASSWORD": null,
                "DB_DRIVER": "mssql",
                "TRUSTED_DB_CON": null,
                "LIST_ERRORS": "true",
                "LIST_RESULTS": null
            },
            "program": "${workspaceFolder}\\index.js"
        }
    ]
}